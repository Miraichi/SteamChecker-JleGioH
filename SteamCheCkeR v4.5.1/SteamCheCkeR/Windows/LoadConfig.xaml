<Window
	x:Class="SteamCheCkeR.Windows.LoadConfig" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:gif="http://wpfanimatedgif.codeplex.com"
	Title="Load Config"
	Height="100"
	Width="123"
	MaxHeight="100"
	MaxWidth="123"
	MinHeight="100"
	MinWidth="123"
	Icon="/mainicon.ico"
	WindowStyle="None"
	AllowsTransparency="True"
	Background="#00FFFFFF"
	Topmost="True"
	WindowStartupLocation="CenterScreen" xmlns:windows="clr-namespace:SteamCheCkeR.Windows"
	Loaded="WindowLoad">
	<Canvas
		Background="#00FFFFFF">
		<Image
			Name="GifInfo"
			ImageBehavior.AnimatedSource="/Images/loadConfig.gif"
			ToolTip="Дождитесь завершения загрузки конфигурации!"
			Height="100"
			Width="119"
			Canvas.Left="4" />
	</Canvas>
</Window>
